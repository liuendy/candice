<?xml version="1.0" encoding="UTF-8"?>

<rule>

    <tableRule name="rule1" forceHit="true">
        <columns>flux_no</columns>
        <dbRuleList>
            <dbRule><![CDATA[
            $stringUtil.substring($FLUX_NO,-2,-1)
            ]]>
            </dbRule>
        </dbRuleList>
        <tbRuleList>
            <tbRule><![CDATA[
            #set($subfix="_"+$stringUtil.substring($FLUX_NO,-2))##
			$!subfix]]>
            </tbRule>
        </tbRuleList>
        <!-- 00-99 100个表,每个表属于哪个结点 Map<Integer,Set<String>> -->
        <tbPrefix>
            <![CDATA[
			      def map = [:];
						for (int i=0; i<10; i++) {
						   def list = [];
							for (int j=0; j<10; j++) {
								list.add("_"+i+""+j);
							}
							 map.put(i,list);
						};
						return map;
				]]>
        </tbPrefix>
    </tableRule>

</rule>
